, auc\n","\n","# Assuming you have your true labels (`y_true`) and predicted probabilities (`y_pred_proba`)\n","\n","# Calculate ROC curve and AUC score\n","fpr, tpr, _ = roc_curve(y_pred_LR, y_test_LR)\n","roc_auc = auc(fpr, tpr)\n","\n","# Define colors\n","yellow = \"#FED101\"\n","green = \"#48CFAE\"\n","\n","# Plot ROC curve\n","plt.figure(figsize=(8, 6))\n","plt.plot(fpr, tpr, color=yellow, label='ROC curve (area = %0.2f)' % roc_auc)\n","plt.plot([0, 1], [0, 1], color=green, linestyle='--', label='Random Guess')\n","plt.xlim([0.0, 1.0])\n","plt.ylim([0.0, 1.05])\n","plt.xlabel('False Positive Rate')\n","plt.ylabel('True Positive Rate')\n","plt.title('ROC Curve')\n","plt.legend(loc=\"lower right\")\n","plt.grid(True)\n","plt.savefig(\"roc_curve.png\", bbox_inches=\"tight\")\n","\n","plt.show()\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":564},"id":"wzkIec86dSR5","executionInfo":{"status":"ok","timestamp":1709070545113,"user_tz":-60,"elapsed":856,"user":{"displayName":"Zakra Chachar","userId":"11031090855160572405"}},"outputId":"042e566b-1a1a-4792-9f92-b95ead086a07"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 800x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["model_LR.coef_"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"5FNzbBQrmW26","executionInfo":{"status":"ok","timestamp":1709071514770,"user_tz":-60,"elapsed":371,"user":{"displayName":"Zakra Chachar","userId":"11031090855160572405"}},"outputId":"9c0eb220-70bc-42a3-ae03-de0a20569059"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([[ 0.23444355, -0.33849373,  0.16235738,  0.31942367, -0.58583124,\n","        -0.04853254, -0.05551765, -0.05722082, -0.13986075,  0.09303139,\n","         0.44600572, -0.35258022, -0.35924022,  0.16526228, -0.00349774,\n","         0.06506774, -0.11480785,  0.06959828, -0.04582103, -0.078087